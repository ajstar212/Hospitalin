<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link rel="stylesheet" href="/style.css" />
    <script src="https://unpkg.com/@lottiefiles/lottie-player@latest/dist/lottie-player.js"></script>
    <title>Hospitalin</title>
  </head>
  <body>
    <section>
      <div id="navbar">
        <a href="/">HOSPITALIN</a>
      </div>
    </section>
    <section class="main">
      <div class="container">
        <div class="lottie">
          <lottie-player
            src="/3/doctors.json"
            background="transparent"
            speed="1"
            style="width: 100%; height: 100%"
            loop
            autoplay
          ></lottie-player>
        </div>
        <div class="links">
          <div class="title">
            <h1>Find Doctors Near You</h1>
          </div>
          <div class="button">
            <a href="/route">Find Route</a>
          </div>
        </div>
      </div>
    </section>
  </body>
</html>
